    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "browser-image-compression": "^2.0.2",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^8.19.2",
    "next": "^16.1.1",
    "next-auth": "^4.24.13",
