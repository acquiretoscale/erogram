    "restore-articles": "node --env-file=.env.local scripts/restore-articles-from-dump.mjs",
    "restore-articles:dry": "node --env-file=.env.local scripts/restore-articles-from-dump.mjs --dry-run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
